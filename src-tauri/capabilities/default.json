{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "opener:default",
    "store:default",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-file",
    "fs:allow-exists",
    "fs:allow-stat",
    "updater:default",
    "process:default",
    "process:allow-restart"
  ]
}
